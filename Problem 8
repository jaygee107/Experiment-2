#include <iostream>

using namespace std;

int main()
{
    int num, prod;
    prod = 0;
    
    cout << "Enter a number from 1 - 10: ";
    cin >> num;
    
    cout << "Multiplication table of: " << num;
    
    for (int i = 1; i <= 10; i++)
        
    {
        cout << num << " X " << i << " = " << (num * i) << endl;
        
    }
    
    return 0;
}


