#include <iostream>

using namespace std;

int main ()

{
    int w, x, y;
    
    w = 0;
    x = 1;
    
    
    cout << "Now showing: The fibonacci sequence" << endl;
    
    for (int i = 0; i <= 21; i++)
    {
        cout << w << ", ";
        
        y = w + x;
        w = x;
        x = y;
        
    }
    return 0;
}

